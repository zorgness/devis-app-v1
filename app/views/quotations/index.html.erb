<h1>Tous mes Devis</h1>

<div class="quotations-container"></div>
<% @quotations.each do |quotation|%>
  <%= link_to quotation_path(quotation)%>
      <div class="card" style="width: 18rem;">
      <div class="card-body">
        <h5 class="card-title"><%= quotation.title%></h5> %></h5>
        <h6 class="card-subtitle mb-2 text-muted"><%= quotation.customer.first_name%> <%= quotation.customer.last_name%></h6> %></h6>
        <p class="card-text"><%= quotation.customer.address%></p>
        <p class="card-text"><%= quotation.date %></p>

      </div>
    </div>
  <% end %>
<% end %>
